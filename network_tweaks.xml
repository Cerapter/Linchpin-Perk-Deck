<?xml version="1.0"?>
<tweaks>
	<tweak version="2" name="settings/network" extension="network_settings">
		<search>
			<network/>
			<rpc/>
		</search>
		<target mode="attach">
			<message name="sync_cohesion_stacks" delivery="reliable" receiver="unit">
				<param type="int" min="0" max="255"/> <!-- Amount of Cohesion stacks. There's *technically* no min-max, but like, be reasonable. -->
				<param type="int" min="0" max="255"/> <!-- Tendency of Cohesion stacks. -->
				<param type="string"/> <!-- That ugly-ass packed peer ID array solution I cooked up. -->
				<param type="bool"/> <!-- Change_tendency. If true, sender's tendency is changed on the receiver side. -->
			</message>
		</target>
	</tweak>
	<tweak version="2" name="settings/network" extension="network_settings">
		<search>
			<network/>
			<rpc/>
		</search>
		<target mode="attach">
			<message name="sync_add_cohesion_stacks" delivery="reliable" receiver="unit">
				<param type="int" min="0" max="255"/> <!-- Amount of Cohesion stacks. -->
				<param type="bool"/> <!-- Go over the tendency? -->
				<param type="string"/> <!-- That ugly-ass packed peer ID array solution I cooked up. -->
			</message>
		</target>
	</tweak>
</tweaks>
